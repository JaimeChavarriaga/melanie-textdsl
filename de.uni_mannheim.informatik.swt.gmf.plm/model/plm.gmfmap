                  href="plm.gmftool#//@palette/@tools.1/@tools.2"/>
              <diagramNode
                  href="plm.gmfgraph#DomainConnection"/>
            </ownedChild>
          </children>
          <compartments>
            <compartment
                href="plm.gmfgraph#DomainElementsComartment"/>
          </compartments>
        </ownedChild>
      </children>
      <compartments>
        <compartment
            href="plm.gmfgraph#ModelCompartment"/>
      </compartments>
    </ownedChild>
