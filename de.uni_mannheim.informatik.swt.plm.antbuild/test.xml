<project name="gmfgen-integration" default="codegen">
    <description>
            Central build file.
    </description>
	
	
	
    <!-- ================================= 
          target: default              
         ================================= -->
    <target name="codegen" depends="depends" description="Central build file.">
		
    	<!-- Delete the generated GMF source -->
		<delete includeemptydirs="true">
	    	<fileset dir="../de.uni_mannheim.informatik.swt.models.plm.diagram/src" includes="**/*"/>
		</delete>
    	
    	<ant antfile="org.eclipse.gmf.xpand.ant/examples/gmfgen-run.xml" target="main">
			<property name="input-gmfgen-model-uri" value="../de.uni_mannheim.informatik.swt.gmf.plm/model/plm.gmfgen"></property>
		</ant>
    	
    </target>

    <!-- - - - - - - - - - - - - - - - - - 
          target: depends                      
         - - - - - - - - - - - - - - - - - -->
    <target name="depends">
    	
    </target>

</project>
