          href="plm.gmfgraph#Ontology"/>
      <children>
        <containmentFeature
            href="../../de.uni_mannheim.informatik.swt.models.plm/model/PLM.ecore#//Ontology/models"/>
        <ownedChild>
          <domainMetaElement
              href="../../de.uni_mannheim.informatik.swt.models.plm/model/PLM.ecore#//Model"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
